	<script type="text/javascript">
  var xmlHttp;
	
  //ajax连接
  function getMoreContents() {
      var contents = document.getElementById("as-search-input");
      if (contents.value != "") {
    	 	 	document.getElementById("popDiv").style.display="block";
	    	  	console.log(contents.value);
	    	  	
	    	  	//该对象是实现异步提交的核心
	         	xmlHttp = createXmlHttp();
	          	
          		//向服务器发送数据,escape函数是编码函数，将内容编码为ascll传输，以防中文乱码
    	   		var url = "../support?byword="+contents.value;
          		//true表示脚本会在send方法之后继续执行，而不用等待来自服务器的响应（异步）
          		xmlHttp.open("get", url, true)
				console.log("调用open方法跳转到servlet");
          		//xmlHttp绑定回调函数，在对象状态发生改变时调用。有0-4五个状态，一般只关心4（提交完成）
          		//不能带参数，不能带括号
          		xmlHttp.onreadystatechange = callBack;
          		xmlHttp.send(null);
    	  
      }else {
    	  //clearData();
    	  document.getElementById("popDiv").style.display="none";
          return;
      }
    
  }
  
  
	//根据不同的浏览器获得xmlHttp对象
	 
	 function createXmlHttp() {
	      var xmlHttp;
	
	      //一般的浏览器都支持这种方式来获取该对象
	      if (window.XMLHttpRequest) {
	          xmlHttp = new XMLHttpRequest();
	          console.log("创建了xmlHttp对象");
	      }
	
	      //考虑浏览器的兼容性,ie就是他妈的事多
	      if (window.ActiveXObject) {
	          xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
	
	          //虽然该浏览器支持ActiveXObject，但是不支持Microsoft.XMLHTTP
	          if (!xmlHttp) {
	              xmlHttp = new ActiveXObject("Msxml2.XMLHTTP");
	          }
	      }
	      return xmlHttp;
	  } 

	//获得数据
	  function callBack() {
	          if (xmlHttp.readyState == 4) {
	        	  console.log("业务层响应成功");
	              //200表示响应成功，404表示未找到，500表示服务器内部错误
	              if (xmlHttp.status == 200) {
	            	  
	            	  console.log("服务器响应成功");
	                  //交互成功，获得响应数据
	                  var result = xmlHttp.responseText;
	                  console.log("获得响应数据");
	                  console.log(result);
	                   //获得显示的div
	                  var sobj=document.getElementById("popDiv");
	                  //清空div内容
	  				  sobj.innerHTML="";
	  				  var str =result.split("-");
	  				  
	  				  var suggest="";
	  				  if(str[0].length>0){
	  					console.log("获取数据库数据成功");
	  				  for(i=0;i<str.length-1;i++){
	  						suggest+="<div onmouseover=this.className='suggest_link_over' ";
	  						suggest+="onmouseout=this.className='suggest_link' ";
	  						suggest+="onclick='javascript:setSearch(this.innerHTML);'";
	  						suggest+="class='suggest_link'><a href='#' style='text-decoration: none;' >"+str[i]+"</a></div>";
	  					}
	  					sobj.innerHTML=suggest;
	  					console.log("设置div内容成功");
	  					document.getElementById("popDiv").style.display="block";	//显示DIV
	  				}else{
	  					console.log("获取数据库数据失败");
	  					document.getElementById("popDiv").style.display="none"; //隐藏DIV
	  				}      
	              }
	          }
	      }                                                                          
	  	function setSearch(innerHTML){
			document.getElementById("txtSearch").value = innerHTML;
			document.getElementById("suggest").style.display="none";
		}
		
	 </script>
	
	
